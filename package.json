{
  "name": "ai-consulting-registration",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/client/index.tsx --bundle --outfile=dist/bundle.js --loader:.tsx=tsx --loader:.ts=ts --loader:.css=css --target=es2020 --minify",
    "build:dev": "esbuild src/client/index.tsx --bundle --outfile=dist/bundle.js --loader:.tsx=tsx --loader:.ts=ts --loader:.css=css --target=es2020 --sourcemap",
    "dev": "tsx --watch src/server.ts",
    "start": "node --import tsx src/server.ts"
  },
  "dependencies": {
    "express": "^4.22.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.32",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "esbuild": "^0.27.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
